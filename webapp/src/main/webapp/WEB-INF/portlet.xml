<portlet-app version="2.0" xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">

	<portlet>
     <portlet-name>Valek</portlet-name>
     <display-name xml:lang="EN">Valek Juzu Application</display-name>
     <portlet-class>org.exoplatform.samples.Valek</portlet-class>
     <init-param>
       <name>juzu.app_name</name>
       <value>org.exoplatform.samples</value>
     </init-param>
      <init-param>
      <name>juzu.run_mode</name>
      <value>prod</value>
    </init-param>
     <supports>
       <mime-type>text/html</mime-type>
     </supports>
     <portlet-info>
       <title>Test Valek</title>
        <keywords>valek</keywords>
     </portlet-info>
   </portlet>
   
</portlet-app>
